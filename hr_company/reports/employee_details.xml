	<template id="report_companys">
		<t t-call="web.html_container">
			<t t-call="web.internal_layout">
				<t t-foreach="docs" t-as="doc">
                    <h3>Previous Company</h3>
                    <div class="page">
                        <div class="oe_structure"/>
                        <div class="row">
                            <div style="margin:auto;">
                            </div>
                            <br></br>
                            <div style="margin-right:700px;">
                                <table class="table table-condensend" style="border: 3px solid black !important;">
                                    <tr>
                                        <th>Name Of Company:</th><td style="text-align:center;"><t t-esc="doc.name" /></td>
                                    </tr>
                                    <tr>
                                        <th>Gender:</th><td><t t-esc="doc.gender" /></td>
                                    </tr>
                                    <tr>
                                        <th>Currency:</th><td><t t-esc="doc.currency_id" /></td>
                                    </tr>
                                    <tr>
                                        <th>Phone:</th><td><t t-esc="doc.phone" /></td>
                                    </tr>
                                    
                                </table>
                            </div>
                        </div>
                    </div>
				</t>
			</t>
		</t>
	</template>
</odoo>
